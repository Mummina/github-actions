{
  "name": "qyrus_cloudbridge",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "inspect": "electron --inspect=3000 .",
    "pack-mac": "electron-builder -m",
    "pack-win": "electron-builder -w",
    "pack-win-build": "electron-builder --win --publish always ",
    "pack-mac-build": "electron-builder --mac --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QQyrus/QCloudBridge"
  },
  "build": {
    "appId": "com.electron.electron-rp",
    "icon": "./assets/img/logo-min.png",
    "directories": {
      "output": "build"
    },
    "productName": "QloudBridge",
    "mac": {
      "target": [
        "dmg"
      ],
      "identity": null,
      "category": "public.app-category.developer-tools",
      "cscInstallerLink": "file:///app/assets/keys.p12",
      "cscKeyPassword": "",
      "publish": [
        {
          "owner": "QQyrus",
          "repo": "QCloudBridge",
          "provider": "github",
          "private": false,
          "protocol": "https"
        }
      ]
    },
    "dmg": {
      "background": null,
      "icon": "./assets/img/logo-min.png",
      "iconSize": 80,
      "window": {
        "width": 544,
        "height": 408
      },
      "contents": [
        {
          "x": 450,
          "y": 280
        },
        {
          "x": 450,
          "y": 170,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./assets/img/logo-min.png",
      "publish": [
        {
          "owner": "QQyrus",
          "repo": "QCloudBridge",
          "provider": "github",
          "private": false,
          "protocol": "https"
        }
      ]
    }
  },
  "author": "qyrus.com",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.9",
    "electron-builder": "^23.3.3"
  },
  "dependencies": {
    "ag-grid-community": "^28.2.1",
    "ag-grid-enterprise": "^28.2.1",
    "check-internet-connected": "^2.0.6",
    "csvtojson": "^2.0.10",
    "detect-browsers": "^4.0.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "ip": "^1.1.8",
    "node-wget": "^0.4.3",
    "selenium-webdriver": "^4.6.1",
    "sudo-prompt": "^9.2.1",
    "systeminformation": "^5.12.14"
  }
}
